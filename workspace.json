{
  "version": 1,
  "projects": {
    "nx-python": {
      "root": "packages/nx-python",
      "sourceRoot": "packages/nx-python/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/nx-python/tsconfig.lib.json",
              "packages/nx-python/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/nx-python/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/nx-python/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/packages/nx-python"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nx-python",
            "tsConfig": "packages/nx-python/tsconfig.lib.json",
            "packageJson": "packages/nx-python/package.json",
            "main": "packages/nx-python/src/index.ts",
            "assets": [
              "packages/nx-python/*.md",
              {
                "input": "./packages/nx-python/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-python",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/nx-python",
                "glob": "builders.json",
                "output": "."
              }
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "nx-python-e2e": {
      "projectType": "application",
      "root": "e2e/nx-python-e2e",
      "sourceRoot": "e2e/nx-python-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-python:build",
            "npmPackageName": "@nx-python/nx-python",
            "pluginOutputPath": "dist/packages/nx-python",
            "jestConfig": "e2e/nx-python-e2e/jest.config.js",
            "tsSpecConfig": "e2e/nx-python-e2e/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}